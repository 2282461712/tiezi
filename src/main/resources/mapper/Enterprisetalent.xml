<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.szxs.dao.EpDao">

    <select id="queryEp" resultType="com.szxs.entity.Enterprisetalent">
      SELECT d.name "department.name",e.* FROM department d,enterpriseTalent e WHERE e.`departmentId`=d.id
    </select>
</mapper>